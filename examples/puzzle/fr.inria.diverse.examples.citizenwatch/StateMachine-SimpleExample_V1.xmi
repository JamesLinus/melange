<?xml version="1.0" encoding="ASCII"?>
<CompleteFSM:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:CompleteFSM="http://CompleteFSM"
    xsi:schemaLocation="http://CompleteFSM ../fr.inria.diverse.examples.puzzle.fsm.CompleteFSM/composition-gen/CompleteFSM.ecore">
  <subvertex
      xsi:type="CompleteFSM:Pseudostate"
      name="Initial"
      outgoing="//@transitions.0"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="A"
      incoming="//@transitions.0"
      outgoing="//@transitions.1"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="B"
      incoming="//@transitions.2"
      outgoing="//@transitions.4"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="C"
      incoming="//@transitions.3"
      outgoing="//@transitions.5"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="D"
      incoming="//@transitions.6"/>
  <subvertex
      xsi:type="CompleteFSM:Pseudostate"
      name="Fork"
      incoming="//@transitions.1"
      outgoing="//@transitions.2 //@transitions.3"
      kind="fork"/>
  <subvertex
      xsi:type="CompleteFSM:Pseudostate"
      name="Join"
      incoming="//@transitions.4 //@transitions.5"
      outgoing="//@transitions.6"
      kind="join"/>
  <transitions
      name="InitialToA"
      target="//@subvertex.1"
      source="//@subvertex.0">
    <trigger
        expression="a"/>
  </transitions>
  <transitions
      name="AToFork"
      target="//@subvertex.5"
      source="//@subvertex.1">
    <trigger
        expression="fork"/>
  </transitions>
  <transitions
      name="ForkToB"
      target="//@subvertex.2"
      source="//@subvertex.5"/>
  <transitions
      name="ForkToC"
      target="//@subvertex.3"
      source="//@subvertex.5"/>
  <transitions
      name="BToJoin"
      target="//@subvertex.6"
      source="//@subvertex.2"/>
  <transitions
      name="CToJoin"
      target="//@subvertex.6"
      source="//@subvertex.3"/>
  <transitions
      name="JoinToD"
      target="//@subvertex.4"
      source="//@subvertex.6">
    <trigger
        expression="d"/>
  </transitions>
</CompleteFSM:StateMachine>
