<?xml version="1.0" encoding="ASCII"?>
<CompleteFSM:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:CompleteFSM="http://CompleteFSM"
    xsi:schemaLocation="http://CompleteFSM ../fr.inria.diverse.examples.puzzle.fsm.CompleteFSM/composition-gen/CompleteFSM.ecore"
    name="BigStateMachine">
  <regions name="MainRegion">
    <subvertex
        xsi:type="CompleteFSM:Pseudostate"
        name="Initial"
        outgoing="//@regions.0/@transitions.0"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="A"
        incoming="//@regions.0/@transitions.0"
        outgoing="//@regions.0/@transitions.1"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="B"
        incoming="//@regions.0/@transitions.1"
        outgoing="//@regions.0/@transitions.2"/>
    <subvertex
        xsi:type="CompleteFSM:Pseudostate"
        name="Split"
        incoming="//@regions.0/@transitions.2"
        outgoing="//@regions.0/@transitions.3 //@regions.0/@transitions.4"
        kind="junction"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="C"
        incoming="//@regions.0/@transitions.3"
        outgoing="//@regions.0/@transitions.5"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="D"
        incoming="//@regions.0/@transitions.4"
        outgoing="//@regions.0/@transitions.6"/>
    <subvertex
        xsi:type="CompleteFSM:Pseudostate"
        name="Merge"
        incoming="//@regions.0/@transitions.5 //@regions.0/@transitions.6"
        outgoing="//@regions.0/@transitions.7"
        kind="junction"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="E"
        incoming="//@regions.0/@transitions.7"/>
    <transitions
        name="InitialToA"
        target="//@regions.0/@subvertex.1"
        source="//@regions.0/@subvertex.0"/>
    <transitions
        name="AToB"
        target="//@regions.0/@subvertex.2"
        source="//@regions.0/@subvertex.1">
      <trigger
          expression="e1"/>
    </transitions>
    <transitions
        name="BToSplit"
        target="//@regions.0/@subvertex.3"
        source="//@regions.0/@subvertex.2"/>
    <transitions
        name="SplitToC"
        target="//@regions.0/@subvertex.4"
        source="//@regions.0/@subvertex.3">
      <trigger
          expression="e2"/>
    </transitions>
    <transitions
        name="SplitToD"
        target="//@regions.0/@subvertex.5"
        source="//@regions.0/@subvertex.3">
      <trigger
          expression="e3"/>
    </transitions>
    <transitions
        name="CToMerge"
        target="//@regions.0/@subvertex.6"
        source="//@regions.0/@subvertex.4"/>
    <transitions
        name="DToMerge"
        target="//@regions.0/@subvertex.6"
        source="//@regions.0/@subvertex.5"/>
    <transitions
        name="MergeToE"
        target="//@regions.0/@subvertex.7"
        source="//@regions.0/@subvertex.6">
      <trigger
          expression="e4"/>
    </transitions>
  </regions>
</CompleteFSM:StateMachine>
