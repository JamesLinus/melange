<?xml version="1.0" encoding="ASCII"?>
<CompleteFSM:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:CompleteFSM="http://CompleteFSM"
    xsi:schemaLocation="http://CompleteFSM ../fr.inria.diverse.examples.puzzle.fsm.CompleteFSM/composition-gen/CompleteFSM.ecore">
  <regions name="Main">
    <subvertex
        xsi:type="CompleteFSM:Pseudostate"
        name="InitialXYZ"
        outgoing="//@regions.0/@transitions.0"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="Displays"
        incoming="//@regions.0/@transitions.0 //@regions.0/@transitions.3"
        outgoing="//@regions.0/@transitions.1 //@regions.0/@transitions.2 //@regions.0/@transitions.4"/>
    <subvertex
        xsi:type="CompleteFSM:State"
        name="Alarms"
        incoming="//@regions.0/@transitions.4"
        outgoing="//@regions.0/@transitions.3">
      <ownedRegions
          name="RegionAlarms">
        <subvertex
            xsi:type="CompleteFSM:State"
            name="Alarm1"
            incoming="//@regions.0/@transitions.1"/>
        <subvertex
            xsi:type="CompleteFSM:State"
            name="Alarm2"
            incoming="//@regions.0/@transitions.2"/>
      </ownedRegions>
    </subvertex>
    <transitions
        name="InitialToDisplays"
        target="//@regions.0/@subvertex.1"
        source="//@regions.0/@subvertex.0"/>
    <transitions
        name="DisplaysToAlarm1"
        target="//@regions.0/@subvertex.2/@ownedRegions.0/@subvertex.0"
        source="//@regions.0/@subvertex.1">
      <trigger
          expression="T1"/>
    </transitions>
    <transitions
        name="DisplaysToAlarms2"
        target="//@regions.0/@subvertex.2/@ownedRegions.0/@subvertex.1"
        source="//@regions.0/@subvertex.1">
      <trigger
          expression="T2"/>
    </transitions>
    <transitions
        name="AlarmsToDisplays"
        target="//@regions.0/@subvertex.1"
        source="//@regions.0/@subvertex.2">
      <trigger
          expression="a"/>
    </transitions>
    <transitions
        name="DisplaysToAlarms"
        target="//@regions.0/@subvertex.2"
        source="//@regions.0/@subvertex.1">
      <trigger
          expression="T"/>
    </transitions>
  </regions>
</CompleteFSM:StateMachine>
