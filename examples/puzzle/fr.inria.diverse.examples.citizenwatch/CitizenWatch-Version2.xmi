<?xml version="1.0" encoding="ASCII"?>
<CompleteFSM:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:CompleteFSM="http://CompleteFSM"
    xsi:schemaLocation="http://CompleteFSM ../fr.inria.diverse.examples.puzzle.fsm.CompleteFSM/composition-gen/CompleteFSM.ecore">
  <subvertex
      xsi:type="CompleteFSM:Pseudostate"
      name="Initial"
      outgoing="//@transitions.0"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="displays"
      incoming="//@transitions.0 //@transitions.6 //@transitions.7"
      outgoing="//@transitions.1 //@transitions.2 //@transitions.3"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="alarm1"
      incoming="//@transitions.1 //@transitions.4"
      outgoing="//@transitions.6"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="alarm2"
      incoming="//@transitions.2 //@transitions.5"
      outgoing="//@transitions.7"/>
  <subvertex
      xsi:type="CompleteFSM:Pseudostate"
      name="bothAlarms"
      incoming="//@transitions.3"
      outgoing="//@transitions.4 //@transitions.5"
      kind="fork"/>
  <transitions
      name="start"
      target="//@subvertex.1"
      source="//@subvertex.0">
    <trigger
        expression="start"/>
  </transitions>
  <transitions
      name="T1"
      target="//@subvertex.2"
      source="//@subvertex.1">
    <trigger
        expression="T1"/>
  </transitions>
  <transitions
      name="T2"
      target="//@subvertex.3"
      source="//@subvertex.1">
    <trigger
        expression="T2"/>
  </transitions>
  <transitions
      name="T12"
      target="//@subvertex.4"
      source="//@subvertex.1">
    <trigger
        expression="T12"/>
  </transitions>
  <transitions
      name="ForkT1"
      target="//@subvertex.2"
      source="//@subvertex.4"/>
  <transitions
      name="ForkT2"
      target="//@subvertex.3"
      source="//@subvertex.4"/>
  <transitions
      name="aT1"
      target="//@subvertex.1"
      source="//@subvertex.2">
    <trigger
        expression="a"/>
  </transitions>
  <transitions
      name="aT2"
      target="//@subvertex.1"
      source="//@subvertex.3">
    <trigger
        expression="a"/>
  </transitions>
</CompleteFSM:StateMachine>
