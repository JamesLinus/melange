<?xml version="1.0" encoding="ASCII"?>
<CompleteFSM:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:CompleteFSM="http://CompleteFSM"
    xsi:schemaLocation="http://CompleteFSM ../fr.inria.diverse.examples.puzzle.fsm.CompleteFSM/composition-gen/CompleteFSM.ecore">
  <subvertex
      xsi:type="CompleteFSM:Pseudostate"
      name="Initial"
      outgoing="//@transitions.0"/>
  <subvertex
      xsi:type="CompleteFSM:State"
      name="displays"
      incoming="//@transitions.0 //@transitions.3"
      outgoing="//@transitions.1 //@transitions.2"/>
  <subvertex
      xsi:type="CompleteFSM:CompositeState"
      name="alarms"
      outgoing="//@transitions.3">
    <states
        name="alarm 1"
        incoming="//@transitions.1"/>
    <states
        name="alarm 2"
        incoming="//@transitions.2"/>
  </subvertex>
  <transitions
      name="start"
      target="//@subvertex.1"
      source="//@subvertex.0">
    <trigger
        expression="start"/>
  </transitions>
  <transitions
      name="T1"
      target="//@subvertex.2/@states.0"
      source="//@subvertex.1">
    <trigger
        expression="T1"/>
  </transitions>
  <transitions
      name="T2"
      target="//@subvertex.2/@states.1"
      source="//@subvertex.1">
    <trigger
        expression="T2"/>
  </transitions>
  <transitions
      name="a"
      target="//@subvertex.1"
      source="//@subvertex.2">
    <trigger
        expression="a"/>
  </transitions>
</CompleteFSM:StateMachine>
