package $packageName$

import static extension fr.inria.diverse.melange.lib.MetamodelExtensions.*

metamodel $metamodelName$ {
	ecore "platform:/resource/$ecoreFilePath$"
	exactType $metamodelName$MT
}


transformation execute($metamodelName$MT mymodel) {
	val root = mymodel.contents.head as $metamodelName$

	print("root: ")
	println(root)
}

@Main
transformation main() {
	val m1 = $metamodelName$.load("input/Simple.xmi",      $metamodelName$MT)
	
	execute.call(m1)
}
