package mergetest

language MM1 {
	ecore "tests-inputs/metamodels/merge/MM1.ecore"
	exactType mm1MT
}

language MM2 {
	ecore "tests-inputs/metamodels/merge/MM2.ecore"
	exactType mm2MT
}

language SliceMM1{
	slice MM1 on ['A']
	exactType SliceMM1MT
}

language SliceMM2{
	slice MM2 on ['C']
	exactType SliceMM2MT
}