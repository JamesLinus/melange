package ecoretest

import static extension fr.inria.diverse.melange.lib.MetamodelExtensions.*

language Ecore {
	ecore "tests-inputs/metamodels/Ecore.ecore"
	exactType EcoreMT
}

transformation int testListClassifiersCount(EcoreMT m) {
	val root = m.contents.head as ecoretest.ecoremt.EPackage
	return root.EClassifiers.size
}

transformation EcoreMT loadEcore() { return Ecore.load("tests-inputs/metamodels/FSM.ecore", EcoreMT) }

@Main
transformation test() {}
