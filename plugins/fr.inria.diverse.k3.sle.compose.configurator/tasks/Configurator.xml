    <path id="fr.inria.diverse.k3.sle.compose.configurator.classpath">
        <path refid="fr.inria.diverse.k3.sle.compose.configurator.libraryclasspath"/>
    </path>

    <!-- The classpath with only the jars available in the plugin folder of Eclipse -->
    <path id="fr.inria.diverse.k3.sle.compose.configurator.libraryclasspath.alljars">
        <pathelement location="../fr.inria.diverse.k3.sle.compose.configurator/bin"/>
        <fileset dir="${ECLIPSE_HOME}/plugins/" includes="*.jar"/>
    </path>

    <path id="fr.inria.diverse.k3.sle.compose.configurator.classpath.alljars">
        <path refid="fr.inria.diverse.k3.sle.compose.configurator.libraryclasspath.alljars"/>
    </path>

    <!-- The default task with the jars used by this project -->
    <target name="Configurator">
        <java classname="fr.inria.diverse.k3.sle.compose.configurator.common.Configurator" classpathref="fr.inria.diverse.k3.sle.compose.configurator.classpath">
            <arg value="${model}"/>
            <arg value="${target}"/>
        </java>
    </target>

    <!--
        In case of problem you can also try with all the jar available in your Eclipse
        Keep in mind that having two versions of the same jar can create problems
    -->
    <target name="Configurator.alljars">
        <java classname="fr.inria.diverse.k3.sle.compose.configurator.common.Configurator" classpathref="fr.inria.diverse.k3.sle.compose.configurator.classpath">
            <arg value="${model}"/>
            <arg value="${target}"/>
        </java>
    </target>
